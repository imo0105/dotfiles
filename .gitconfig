# Referred to https://qiita.com/peccul/items/90dd469e2f72babbc106
[alias]
    a = add
    b = branch
    bn = rev-parse --abbrev-ref HEAD
    cm = commit
    st = status
    ck = checkout 
    cancel = reset HEAD^ --soft
    unstage = reset HEAD
    stash = stash save
    suba = submodule add
    subit = submodule init
    subup = submodule foreach git pull origin master

